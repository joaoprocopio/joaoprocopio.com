---
import Typography from "~/components/typography.astro"
import { EMAIL_LINK } from "~/constants/links"
---

<div>
  <Typography variant="callout" affect="regular">
    Do you need a Software Engineer?
  </Typography>

  <Typography
    class="mt-1"
    variant="largeTitle"
    affect="emphasized"
    element="a"
    target="_blank"
    href={EMAIL_LINK.HREF}>
    Send me a mail!
  </Typography>
</div>
